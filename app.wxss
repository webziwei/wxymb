.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

page{
      background-color: #f5f6f7;
      font-family: "simsun";
      /* padding-bottom:40px; */
}
.clearfix::after{
      content: "";
      display: block;
      clear:both;
      visibility: hidden;
}
.my_title{
      width:100%;
      height:40px;
      line-height: 40px;
      text-align: center;
      color:#69C3AA;
      font-weight: bold;
      border-bottom:1px dashed #69C3AA;
}
